      "source": [
        "def softmax(x):\n",
        "    \"\"\"Compute the softmax of vector x.\"\"\"\n",
        "    exp_x = np.exp(x)\n",
        "    return exp_x / np.sum(exp_x)\n",
        "\n",
        "# Example usage\n",
        "x = np.array([2.0, 1.0, 0.1])\n",
        "softmax_x = softmax(x)\n",
        "print(softmax_x)\n"
      ]
    }
